DJANGO_SETTINGS_MODULE=nepal_geonode.local_settings
GEONODE_INSTANCE_NAME=geonode
GEONODE_LB_HOST_IP
GEONODE_LB_PORT
DATABASE_URL=postgres://postgres:postgres@db:5432/npl_geonode
DEFAULT_BACKEND_DATASTORE=geonode
GEONODE_DATABASE=npl_geo
GEONODE_DATABASE_PASSWORD=geonode
GEONODE_GEODATABASE=npl_geo_data
GEONODE_GEODATABASE_PASSWORD=geonode
ASYNC_SIGNALS=False
BROKER_URL=amqp://guest:guest@rabbitmq:5672
ALLOWED_HOSTS=['localhost','geonepal.info']
DOCKER_ENV=production
UWSGI_CMD=uwsgi --ini /usr/src/geonode/uwsgi.ini
IS_CELERY=False
C_FORCE_ROOT=1
GEOSERVER_PUBLIC_LOCATION=https://geonepal.info/geoserver/
GEOSERVER_LOCATION=http://geoserver:8080/geoserver/
SITEURL=https://geonepal.info/
COMPOSE_HTTP_TIMEOUT=300
DEFAULT_BACKEND_UPLOADER=geonode.importer
TIME_ENABLED=True
MOSAIC_ENABLED=False
GEOGIG_ENABLED=False

# See https://github.com/geosolutions-it/geonode-generic/issues/28
# to see why we force API version to 1.24
DOCKER_API_VERSION: "1.24"
